{
  "name": "myweishanli/yii2-extjs-rbac",
  "description": "Yii2 ExtJs5 RBAC",
  "keywords": [
    "yii2",
    "extjs5",
    "rbac"
  ],
  "homepage": "https://github.com/myweishanli/yii2-extjs-rbac",
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "source": "https://github.com/myweishanli/yii2-extjs-rbac"
  },
  "authors": [
    {
      "name": "ShanLi Wei",
      "email": "myweishanli@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": ">=2.0.6",
    "bower-asset/rbac-extjs-extend": "dev-master",
    "myweishanli/yii2-extjs-rbac": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "wsl\\rbac\\": ""
    }
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
